{
  "name": "fast-dates",
  "version": "1.1.0",
  "description": "Fastest lib for date working days diff between two dates. Custom exclusions available. ",
  "main": "src/main.js",
  "scripts": {
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mtxr/fast-dates.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "debug": "^3.1.0",
    "jest": "^21.2.1",
    "jest-environment-node-debug": "^2.0.0",
    "standard": "^10.0.3"
  },
  "jest": {
    "automock": false,
    "collectCoverage": false,
    "collectCoverageFrom": [
      "!**/node_modules/**",
      "!**/test/**",
      "**/*.js"
    ],
    "coverageDirectory": "<rootDir>/coverage/",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/test/",
      "<rootDir>/coverage/"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "modulePaths": [
      "<rootDir>"
    ],
    "notify": false,
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/test/*.test.js"
    ]
  },
  "standard": {
    "env": [
      "jest"
    ]
  }
}
